@model List<CadastroUsuarios.POJO.Usuario>

<!DOCTYPE html>
<html>
<head>
    <title>Lista de Usuários</title>
</head>
<body>
    <h1>Lista de Usuários</h1>
    <table>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Telefone</th>
            <th>Sexo</th>
            <th>Data de Nascimento</th>
        </tr>
        @foreach (var usuario in Model)
        {
            <tr>
                <td>@usuario.Id</td>
                <td>@usuario.Nome</td>
                <td>@usuario.Email</td>
                <td>@usuario.Telefone</td>
                <td>@usuario.Sexo</td>
                <td>@usuario.DataNascimento.ToString("dd/MM/yyyy")</td>
            </tr>
        }
    </table>
</body>
</html>
